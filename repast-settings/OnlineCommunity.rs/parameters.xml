<?xml version="1.0" encoding="UTF-8" ?>
<parameters>

	<!-- SIMULATOR SETTINGS -->
	
	<parameter 	name="StopTick" displayName="Stop Tick"
				type="int" 
				defaultValue="5000" 
				isReadOnly="false" 
				converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
				
	<parameter 	name="randomSeed" displayName="Default Random Seed"
				type="int" 
				defaultValue="__NULL__"
				isReadOnly="false" 
				converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
				
	<parameter 	name="Total Comments" displayName="Total Comments"
				type="int" 
				defaultValue="1000" 
				isReadOnly="false" 
				converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	
	<parameter 	name="maxAgents" displayName="Number max agents"
				type="int" 
				defaultValue="10" 
				isReadOnly="false" 
				converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />

	<parameter 	name="Norm Violation Rate" displayName="Norm Violation Rate"
				type="float" 
				defaultValue="0.3" 
				isReadOnly="false" 
				converter="repast.simphony.parameter.StringConverterFactory$FloatConverter" />
	
	<!-- NORM SYNTHESIS SETTINGS -->

	<parameter 	name="NormSynthesisStrategy" isReadOnly="false" displayName="NSM: Norm synthesis strategy (0=CUSTOM/1=IRON/2=SIMON/3=SIMON+/4=LION)" 
				type="int"
				converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
				defaultValue="3" />

	<parameter 	name="NormGeneralisationMode" isReadOnly="false" displayName="NSM: Norm generalisation mode (SHALLOW/DEEP)"
				type="int"
				converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
				defaultValue="1" />

	<parameter 	name="NormGeneralisationStep" isReadOnly="false" displayName="NSM: Norm generalisation step"
				type="int"
				converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
				defaultValue="1" />
	
	<parameter 	name="NormsGenEffThreshold" isReadOnly="false" displayName="NSM: Norm gen. eff. threshold"
				type="double" 
				converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
				defaultValue="-0.1" />
	
	<parameter 	name="NormsGenNecThreshold" isReadOnly="false" displayName="NSM: Norm gen. nec. threshold"
				type="double" 
				converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
				defaultValue="-0.1" />
	
	<parameter 	name="NormsSpecEffThreshold" isReadOnly="false" displayName="NSM: Norm spec. eff. threshold"
				type="double" 
				converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
				defaultValue="0.05" />
	
	<parameter 	name="NormsSpecNecThreshold" isReadOnly="false" displayName="NSM: Norm spec. nec. threshold"
				type="double" 
				converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
				defaultValue="0.1" />
	
	<parameter 	name="NormsSpecThresholdEpsilon" isReadOnly="false" displayName="NSM: Spec. threshold epsilon"
				type="double" 
				converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
				defaultValue="0.025" />
				
	<parameter 	name="NormsMinEvaluations" isReadOnly="false" displayName="NSM: Norms min. evaluations"
				type="int" 
				converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
				defaultValue="20" />
				
	<parameter 	name="NormsPerfRangeSize" isReadOnly="false" displayName="NSM: Norms performance range size"
				type="int" 
				converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
				defaultValue="100" />
				
	<parameter 	name="NormsDefaultUtility" isReadOnly="false" displayName="NSM: Norms default utility"
				type="double" 
				converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
				defaultValue="0.5" />
				
	<parameter 	name="NumTicksToConverge" isReadOnly="false" displayName="NSM: Num. ticks to converge"
				type="long" 
				converter="repast.simphony.parameter.StringConverterFactory$LongConverter"
				defaultValue="1000" />			
	
	<parameter 	name="NormsWithUserId" isReadOnly="false" displayName="NSM: Norms with user id?"
				type="boolean" 
				converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
				defaultValue="true" />
</parameters>
